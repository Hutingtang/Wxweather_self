page {
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 100%;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.text {
  color: white;
  font-size: 16px;
}
.container{
  display:flex;
  flex-direction:column;
}
.header {
  width: 100%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  
}

.icon {
  color: white;
  vertical-align: middle;
}

.curLocation {
  font-size: 20px;
  color: white;
  vertical-align: middle;
}

/* .content {
  width: 100%;
  height:80%;
  flex:1,
} */

.cond_txt {
  font-size: 12px;
  text-align: center;
}

.now_tmp_num {
  font-size: 110rpx;
  text-align: center;
  position:relative;
}

.today_info {
  width: 93%;
  margin-right: 10px;
  margin-left: 10px;
  border-bottom: 1px solid rgba(255,255,255,0.3);
  position: relative;
  padding-bottom: 10px;
}

.today_weekend {
  font-size: 16px;
  display: inline-block;
  margin-right: 32px;
}

.tmp_max {
  position: absolute;
  right: 32px;
  font-size: 16px;
  display: inline-block;
}

.tmp_min {
  position: absolute;
  right: 0;
  display: inline-block;
  font-size: 16px;
  color: gray;
}

.hour_box {
  width: 20%;
  display: inline-flex;
  flex-direction: column;
  align-items: center;
}
.hour_cond_txt{
  margin-top:5px;
  margin-bottom:5px;
}
.scroll-view_X {
  margin-top: 10px;
  padding-bottom:10px;
  overflow: hidden;
  width: 93%;
  margin-right: 10px;
  margin-left: 10px;
  white-space: nowrap;
  border-bottom: 1px solid rgba(255,255,255,0.3);
}
.scroll-view_Y{
  height:80%;
  overflow: hidden;
  white-space: nowrap;
}

.displayInline{
  display: inline-block
}

.next_tmp{
  width: 93%;
  margin-right: 10px;
  margin-left: 10px;
  border-bottom: 1px solid rgba(255,255,255,0.3);
  padding-bottom: 10px
}
.next_tmp_item{
  position: relative;
  margin-top: 10px;
}
.weatherImage{
  position:absolute;
  width:60px;
  height:60px;
  top:-4px;
}

.daily_item{
  border-bottom: 1px solid rgba(255,255,255,0.3);
  display:flex;
}
.daily_item_line1,.daily_item_line2{
  flex:1;
  display:flex;
  flex-direction:column;
}
.daily_item_text{
  font-size:12px;
  color:rgba(255,255,255,0.5);
  margin-bottom:5px;
  margin-top:5px;
}
.daily_item_data{
  margin-bottom:5px;
}
.lifestyle_item{
  padding:5px 0;
  border-bottom: 1px solid rgba(255,255,255,0.3);
  display:flex;
}
.lifestyle_item_line1,.lifestyle_item_line2{
  flex: 1
}